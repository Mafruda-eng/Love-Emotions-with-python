from turtle import *
bgcolor('#202020')
speed(5)

def Goto(x,y):
    up()
    goto(x, y)
    down()

Goto(0,-120)
begin_fill()
pencolor('white')
fillcolor('white')
circle(230)
end_fill()

Goto(55,-60)
begin_fill()
pencolor('#202020')
fillcolor('#202020')
circle(240)
end_fill()

Goto(150, -40)
begin_fill()
pencolor('black')
fillcolor('black')
left(90)
fd(220)
left(83)
fd(150)
left(14)
fd(150)
left(83)
fd(129)
left(32.4)
circle(240,81)
end_fill()

Goto(150,135)
begin_fill()
pencolor('black')
fillcolor('gold')
left(150)
fd(146)
left(97)
fd(23)
left(83)
fd(146)
left(96)
fd(23)
end_fill()

Goto(0, 152)
begin_fill()
pencolor('black')
fillcolor('gold')
left(97)
fd(149)
left(85)
fd(23)
left(95)
fd(149)
left(83)
fd(23)
end_fill()


Goto(145,100)
pencolor('gold')
pensize(5)
left(84)
fd(30)
Goto(95,106)
fd(30)
Goto(42,112)
fd(31)
left(12)
Goto(-5,115)
fd(30)
Goto(-60,110)
fd(30)
Goto(-115,105)
fd(30)

Goto(-145,-180)
pencolor('white')
style = ('arial', 13, 'bold')
write('May this Eid, Purify our hearts with forgiveness! ', font=style)
Goto(-200, -230)
pencolor('gold')
style = ('arial', 30, 'bold')
write('Eid Mubarak!', font=style)
Goto(-50,-270)
pencolor('white')
style = ('fancy', 20, 'bold',)
write('2 0 2 5', font=style)

hideturtle()
done()
